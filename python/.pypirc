[distutils]
index-servers =
    pypi
    testpypi

[pypi]
repository = https://upload.pypi.org/legacy/
username = __token__
# Add your PyPI token here when publishing
# password = your-token-here

[testpypi]
repository = https://test.pypi.org/legacy/
username = __token__
# Add your TestPyPI token here if needed
# password = pypi-test-token